{
	"type": @type,
	"colors": {
		"input.background": @background,
		"input.border": @editor_background,
		"inputOption.activeBorder": @editor_background,
		"dropdown.background": @background,
		"list.focusBackground": @button_background,
		"list.activeSelectionBackground": @button_background,
		"list.inactiveSelectionBackground": @button_background,
		"list.inactiveSelectionForeground": @base_text,
		"list.hoverBackground": @button_background,
		"button.background": @button_background,
		"button.hoverBackground": @editor_background,
		"editor.background": @editor_background,
		"editor.foreground": @base_text,
		"editor.selectionBackground": @selection_background,
		"editor.lineHighlightBackground": @button_background,
		"editor.hoverHighlightBackground": @editor_background,
		"editorCursor.foreground": @cursor,
		"editorWhitespace.foreground": @base_text,
		"peekViewTitle.background": @editor_background,
		"peekViewResult.background": @editor_background,
		"peekViewEditor.background": @background,
		"tab.activeBackground": @editor_background,
		"tab.inactiveBackground": @background,
		"tab.border": @button_background,
		"editorGroupHeader.tabsBackground": @background,
		"statusBar.background": @background,
		"statusBar.noFolderBackground": @background,
		"activityBar.background": @background,
		"activityBarBadge.background": @editor_background,
		"sideBar.background": @background,
		"sideBar.foreground": @comment,
		"sideBarSectionHeader.background": @editor_background,
		"titleBar.activeBackground": @background,
		"statusBar.debuggingBackground": @function_arg,
		"notifications.background": @editor_background
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": @base_text
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": @comment
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": @strings
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": @constants
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": @constants
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": @constants
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": @keywords
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": @keywords
			}
		},
		{
			"name": "Imports",
			"scope": [
				"keyword.control.import",
				"keyword.control.from"
			],
			"settings": {
				"foreground": @keywords,
				"fontStyle": "italic"
			}
		},
		{
			"name": "Python Doc Strings",
			"scope": [
				"string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
				"string.quoted.docstring.multi.python punctuation.definition.string.end.python",
				"string.quoted.docstring.multi.python"
			],
			"settings": {
				"foreground": @comment
			}
		},
		{
			"name": "Python self as param",
			"scope": [
				"variable.parameter.function.language.special.self.python",
				"variable.parameter.function.language.special.cls.python"
			],
			"settings": {
				"foreground": @keywords,
				"fontStyle": "italic"
			}
		},
		{
			"name": "Python self - in function",
			"scope": [
				"variable.language.special.self.python",
				"variable.language.special.cls.python"
			],
			"settings": {
				"foreground": @keywords
			}
		},
		{
			"name": "Python decorators",
			"scope": [
				"punctuation.definition.decorator.python",
				"meta.function.decorator.python",
				"support.type.python"
			],
			"settings":{ "foreground": @class_names }
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"foreground": @data_types,
				"fontStyle": "italic"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class",
			"settings": {
				"foreground": @class_names
			}
		},
		{
			"name": "Inherited class",
			"scope": [
				"entity.other.inherited-class",
				"keyword.control.flow.js"
			],
			"settings": {
				"foreground": @class_names
			}
		},
		{
			"name": "Function name/call",
			"scope": [
				"entity.name.function",
				"meta.function-call.generic",
				"meta.function-call.js"
			],
			"settings": {
				"fontStyle": "",
				"foreground": @class_names
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"foreground": @function_arg
			}
		},
		{
			"name": "Arguments",
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": @function_arg
			}
		},
		{
			"name": "Tag name",
			"scope": [
				"entity.name.tag",
				"support.type.property-name.json",
				"keyword.control.flow.js"
			],
			"settings": {
				"fontStyle": "",
				"foreground": @keywords
			}
		},
		{
			"name": "Tag attribute",
			"scope": [
				"entity.other.attribute-name",
				"variable.language.this.js"
			],
			"settings": {
				"fontStyle": "",
				"foreground": @class_names
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": @class_names
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": @class_names
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": @keywords
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": "",
				"foreground": @other_variable
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"background": @keywords,
				"fontStyle": "",
				"foreground": @error_state
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": @constants,
				"foreground": @error_state
			}
		},
		{
			"name": "JSON String",
			"scope": "meta.structure.dictionary.json string.quoted.double.json",
			"settings": {
				"foreground": @strings
			}
		},
		{
			"name": "JSON String",
			"scope": "meta.property-name.dictionary.json",
			"settings": {
				"foreground": @data_types
			}
		},
		{
			"name": "diff.header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": @diff_header
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": @keywords
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": @class_names
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": @strings
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": @find_in_file_line_numbers
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": @decorators
			}
		},
		{
			"name": "Decorators",
			"scope": "tag.decorator.js tag.decorator.js punctuation.definition.tag.js",
			"settings": {
				"foreground": @decorators
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.array",
				"punctuation.definition.structure"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": @strings
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": "source.js constant.other.object.key.js string.unquoted.label.js",
			"settings": {
				"foreground": @data_types
			}
		},
		{
			"name": "js string template variable",
			"scope": [
				"meta.template.expression variable",
				"meta.template.expression meta.brace.round",
				"meta.template.expression punctuation.accessor"
			],
			"settings": {
				"fontStyle": "",
				"foreground": @base_text
			}
		},
		{
			"name": "js string template brackets",
			"scope": "meta.template.expression punctuation.definition.template-expression",
			"settings": {
				"fontStyle": "",
				"foreground": @class_names
			}
		},
		{
			"name": "SASS name",
			"scope": "entity.name.selector",
			"settings": {
				"foreground": @data_types
			}
		},
		{
			"name": "SASS LESS variables",
			"scope": [
				"variable.less",
				"variable.ref.sass",
				"variable.decl.sass"
			],
			"settings": {
				"foreground": @strings
			}
		},
		{
			"name": "TS annotations",
			"scope": "annotation.parameters.ts",
			"settings": {
				"foreground": @class_names,
				"fontStyle": ""
			}
		},
		{
			"name": "TS annotations",
			"scope": [
				"annotation.function.name.ts",
				"annotation.function.primitive.support.ts",
				"annotation.entity.parameters.ts"
			],
			"settings": {
				"foreground": @keywords,
				"fontStyle": ""
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": @keywords
			}
		},
		{
			"name": "Markup Quote",
			"scope": [
				"markup.quote",
				"markup.bold.markdown"
			],
			"settings": {
				"foreground": @class_names
			}
		},
		{
			"name": "Markup Quote",
			"scope": [
				"punctuation.definition.string.begin.markdown",
				"punctuation.definition.string.end.markdown",
				"punctuation.definition.metadata.markdown"
			],
			"settings": {
				"foreground": @base_text
			}
		},
		{
			"name": "Markup Quote",
			"scope": [
				"string.other.link.description.markdown",
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": @keywords
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": @strings
			}
		},
		{
			"name": "code",
			"scope": [
				"fenced_code.block.language",
				"punctuation.definition.markdown"
			],
			"settings": {
				"foreground": @function_arg
			}
		},
		{
			"name": "Markup Styling",
			"scope": "markup.bold, markup.italic",
			"settings": {
				"foreground": @markdown_bold
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": @function_arg
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": {
				"fontStyle": "",
				"foreground": @data_types
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": [
				"markup.underline.link.image.markdown",
				"markup.underline.link.markdown"
			],
			"settings": {
				"fontStyle": "",
				"foreground": @data_types
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "",
				"foreground": @data_types
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": @token_info
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": @token_warn
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": @token_error
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": @token_debug
			}
		}
	]
}